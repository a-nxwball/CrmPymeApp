{
  "name": "Feature Branch Protection",
  "description": "Protecci√≥n para branches de nuevas funcionalidades",
  "target": "branch",
  "enforcement": "active",
  "conditions": {
    "ref_name": {
      "include": [
        "feature/*"
      ]
    }
  },
  "rules": [
    {
      "type": "pull_request",
      "parameters": {
        "required_approving_review_count": 1
      }
    }
  ]
} 